{
    "name": "snap-ag/administration",
    "description": "Administration frontend for the SnapStore Core",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Shopware\\Administration\\": ""
        }
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "issues": "https://issues.snapadmin.net",
        "forum": "https://forum.snapadmin.net",
        "wiki": "https://developer.snapadmin.net",
        "docs": "https://developer.snapadmin.net",
        "chat": "https://slack.snapadmin.net"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.6.x-dev",
            "dev-trunk": "6.6.x-dev"
        }
    },
    "require": {
        "php": "~8.2.0 || ~8.3.0",
        "doctrine/dbal": "^3.7",
        "shopware/core": "*",
        "symfony/framework-bundle": "~7.0.0",
        "symfony/http-foundation": "~7.0.0",
        "symfony/routing": "~7.0.0",
        "symfony/mime": "~7.0.0"
    }
}
